@inherits UmbracoViewPage<Lecoati.LeBlender.Extension.Models.LeBlenderModel>
@using FormPackage6.Core.DomainModel.Form;

@{
    if (Model != null)
    {
        Field field = AutoMapper.Mapper.Map<Field>(Model.Items.FirstOrDefault());
        <div class="manaoformBackoffice">
            <div class="form-field">
                <label class="field-label">@field.Name</label>
                <div class="dragdrop-box clearfix">
                    <div class="dragdrop-box-input">
                        <span class="dragdrop-box-placeholder">@field.Placeholder</span>
                    </div>
                </div>
            </div>
        </div>
 
    }
}
